package matches;

import java.io.IOException;
import java.io.InputStream;

public class zhengze {
	public static void main(String[] args) {
		//java从控制台读取
		int inScanner = System.in.read();
		try {
			System.out.println(inScanner.read());
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
}
