package com.droider.hello3;

import java.security.InvalidKeyException;
import java.security.Key;
import java.security.NoSuchAlgorithmException;

import javax.crypto.BadPaddingException;
import javax.crypto.Cipher;
import javax.crypto.IllegalBlockSizeException;
import javax.crypto.NoSuchPaddingException;
import javax.crypto.spec.SecretKeySpec;

import android.util.Base64;

public class decode {
	   private static final byte[] a;

	    static {
	       a= new byte[]{76, 82, 83, 65, 78, 74, 85, 73, 83, 84, 72, 69, 82, 65, 74, 65};
	    }
	    
	  private static Key a() {
	        return new SecretKeySpec(a, "AES");
	    }
	public static String b(String arg3) throws IllegalBlockSizeException, BadPaddingException {
		Key v0 = a();
		Cipher v1 = null;
		try {
			v1 = Cipher.getInstance("AES");
		} catch (NoSuchAlgorithmException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		} catch (NoSuchPaddingException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		try {
			v1.init(2, v0);
		} catch (InvalidKeyException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		return new String(v1.doFinal(Base64.decode(arg3, 0)));
	}
}
