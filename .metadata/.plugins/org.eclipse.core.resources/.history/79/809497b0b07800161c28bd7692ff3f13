package matches;

import java.io.IOException;

public class zhengze {
	public static void main(String[] args) {
		// java从控制台读取
		int i = 0;
		try {
			// 读取返回的是byte数组
			i = System.in.read();
		} catch (IOException e1) {
			// TODO Auto-generated catch block
			e1.printStackTrace();
		}
		System.out.println(Char(i));
	}
}
